<%- include('./partials/header.ejs') %> 


<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h3 class="text-center display-4 my-4">Oops! Something went wrong.</h3>
            <% if (locals.details) { %>
                <div class="alert alert-info alert-dismissible fade show">
                    <button type="button" class="close" data-dismiss="alert">&times;</button>
                    <%= details %>
                </div>
            <% } %>
        </div>
    </div>
</div>


