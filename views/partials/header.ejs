<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
  <link href="https://fonts.googleapis.com/css?family=Exo:700&display=swap" rel="stylesheet">
  <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.6/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js"></script>
  <script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous"></script>
  <title>Proofpoint - Customer Profile</title>
  <!-- local scripts & styles -->
  <!-- <link rel="stylesheet" type="text/css" href="/stylesheets/app.css">
  <script type="text/javascript" src="/scripts/app.js"></script> -->

  <style media="screen">
    .navbar-brand {
      color: white;
    }

    .navbar {
      background-color: #343A40;
    }

    .custab {
      border: 1px solid #cccccc;
      padding: 5px;
      margin-top: 2%;
      box-shadow: 0.5px 0.5px 0.5px #cccccc;
      transition: 0.5s;
    }

    .custab:hover {
      box-shadow: 3px 3px 0px transparent;
      transition: 0.5s;
    }

    .container {
      max-width: 90%;
    }

    .input-group>.input-group-prepend {
    flex: 0 0 35%;
    }
    .input-group .input-group-text {
        width: 100%;
    }

    body { 
      padding-top: 90px; }


    input.required:placeholder-shown {
      border: 1px solid black;
    }
   
    select:has(option:selection[value=""]) {
      border: 1px solid red;
    }

    #godown{
      height: 60px;}



  </style>
</head>

<body>
  <nav class="navbar fixed-top">
    <a class="navbar-brand" href="/index">
      <img src="/images/Proofpoint-logo-Reversed.png" alt="Proofpoint Logo" style="max-height: 70px;">
    </a>

    <% if (user) { %>
        <form action="./logout" method="POST">
          <button class="btn btn-outline-light float-right">Logout <%= user %></button>
        </form>
    <% } %>
    
  </nav>
  <br>